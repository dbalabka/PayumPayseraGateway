{
    "name": "plumtreesystems/payum-paysera-gateway",
    "type": "library",
    "description": "This is a payum library used for paysera payments",
    "keywords": ["payment", "paysera", "payum", "gateway"],
    "homepage": "https://github.com/PlumTreeSystems/PayumPayseraGateway",
    "license": "MIT",
    "authors": [
        {
            "name": "Mykolas Vitkus",
            "email": "mykolasvitkus@gmail.com"
        },
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/AuthorizeNetAim/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "payum/core": "^1.5",
        "webtopay/libwebtopay": "^3"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^2",
        "phpunit/phpunit": "^8"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "PTS\\Paysera": "" }
    },
    "target-dir": "PTS/Paysera",
    "extra": {
        "branch-alias": {
            "dev-development": "0.4-dev"
        }
    }
}
